{
  "name": "http-lean",
  "version": "0.0.1",
  "description": "‚ú® Tiny JavaScript Server üåê",
  "author": "Dennis Ollhoff <contact@nyxb.xyz>",
  "license": "MIT",
  "hompage": "https://üíªnyxb.ws",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyxblabs/http-lean.git"
  },
  "bugs": {
    "url": "https://github.com/nyxblabs/http-lean/issues"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "buildkarium",
    "dev": "vitest",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs .",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix",
    "play": "dynot ./playground/index.ts",
    "profile": "0x -o -D .profile -P 'autocannon -c 100 -p 10 -d 40 http://localhost:$PORT' ./playground/server.cjs",
    "release": "nyxr test && nyxr build && nyxlx changelogen@latest --release && pnpm publish && git push --follow-tags",
    "test": "nyxr lint && vitest run --coverage"
  },
  "dependencies": {
    "esnext-cookie": "^0.0.2",
    "nyxdefaults": "^0.0.3",
    "nyxjson": "^0.0.3",
    "iron-webcrypto": "^0.7.0",
    "radix-rapid": "^0.0.2",
    "url-ops": "^0.0.4",
    "cryptonyx": "^0.0.2"
  },
  "devDependencies": {
    "@nyxb/eslint-config": "0.0.53",
    "0x": "^5.5.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.4",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-c8": "^0.31.0",
    "autocannon": "^7.11.0",
    "connect": "^3.7.0",
    "eslint": "^8.40.0",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "dynot": "^0.1.8",
    "earlist": "^0.0.3",
    "fetch-for-all": "^0.0.3",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4",
    "buildkarium": "^0.0.2",
    "vitest": "^0.31.0"
  },
  "files": [
    "dist"
  ],
  "keywords": []
}
